{
    "builds": [{
        "src": "kvartiri_bg/wsgi.py",
        "use": "@vercel/python",
        "config": { "maxLambdaSize": "15mb", "runtime": "3.9"}
    }],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "kvartiri_bg/wsgi.py"
        }
    ],
    "rewrites": [{ "source": "/(.*)", "destination": "/home.html" }]
}